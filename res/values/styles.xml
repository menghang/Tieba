<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="table_cell">
        <item name="android:gravity">center</item>
    </style>
    <style name="table_cell_lable" parent="table_cell">
        <item name="android:textSize">14dip</item>
        <item name="android:textColor">#000000</item>
        <item name="android:shadowColor">#66666f</item>
        <item name="android:shadowDx">2</item>
        <item name="android:shadowDy">2</item>
        <item name="android:shadowRadius">2</item>
        <item name="android:gravity">center_horizontal</item>
    </style>
    <style name="table_cell_value" parent="table_cell">
        <item name="android:background">#33000000</item>
        <item name="android:textColor">#000000</item>
        <item name="android:shadowColor">#ffffff</item>
        <item name="android:shadowDx">0</item>
        <item name="android:shadowDy">0</item>
        <item name="android:shadowRadius">2</item>
    </style>
    <style name="bt_pressed" parent="table_cell">
        <item name="android:background">#33000000</item>
    </style>
    <style name="bt_default" parent="table_cell">
        <item name="android:background">#330000ff</item>
    </style>
</resources>